<Window x:Class="EELBALL_TRACKER.Views.LeaderboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EELBALL_TRACKER.Views"
        mc:Ignorable="d"
        Title="LeaderboardWindow" Height="255" Width="365">
    <Grid>
        <Grid.Background>
            <RadialGradientBrush>
                <GradientStop Color="#FFFF9300"/>
                <GradientStop Color="#FFB046C9" Offset="1"/>
            </RadialGradientBrush>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="34"/>
            <RowDefinition Height="205*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Margin="5,5,4,5">
            <Label Content="LE" Padding="-1"/>
            <TextBlock Text="EEL" Foreground="Red"/>
            <Label Content="DERBOARD" Padding="-1"/>
        </StackPanel>
        
    </Grid>
</Window>
